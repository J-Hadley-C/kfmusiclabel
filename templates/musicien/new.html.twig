{% extends 'base.html.twig' %}

{% block title %}Créer un Musicien{% endblock %}

{% block body %}
<h1>Créer un Nouveau Musicien</h1>

{{ form_start(form) }}
    {{ form_row(form.nom) }}
    {{ form_row(form.instrument) }}
    {{ form_row(form.genre_musical) }}
    {{ form_row(form.photo) }}
    {{ form_row(form.email) }}
    {{ form_row(form.city) }}
    <button type="submit">Enregistrer</button>
{{ form_end(form) }}
{% endblock %}
